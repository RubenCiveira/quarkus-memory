# https://projectlombok.org/features/configuration

config.stopBubbling = true
# Copy the Value and Qualifier annotation of Spring from the instance variables 
# to the constructor see https://github.com/rzwitserloot/lombok/issues/745
lombok.copyableAnnotations += org.springframework.beans.factory.annotation.Qualifier
lombok.copyableAnnotations += org.springframework.beans.factory.annotation.Value
lombok.copyableAnnotations += org.springframework.beans.factory.annotation.Autowired
lombok.copyableAnnotations += jakarta.inject.Named
lombok.copyableAnnotations += org.eclipse.microprofile.config.inject.ConfigProperty
lombok.copyableAnnotations += org.eclipse.microprofile.rest.client.inject.RestClient
lombok.copyableAnnotations += io.quarkus.qute.Location
lombok.copyableAnnotations += jakarta.batch.api.BatchProperty
lombok.addLombokGeneratedAnnotation = true
